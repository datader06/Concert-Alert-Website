# Example environment variables
# Copy this file to .env and fill in your actual values

# Spotify API
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Ticketmaster API
TICKETMASTER_API_KEY=your_ticketmaster_api_key_here
TICKETMASTER_API_URL=https://app.ticketmaster.com/discovery/v2

# MusicBrainz API
MUSICBRAINZ_USER_AGENT=SpotifyClone/1.0 (your_email@example.com)

# Cache TTL (in seconds)
ARTIST_CACHE_TTL=3600
ALBUM_CACHE_TTL=3600
CONCERT_CACHE_TTL=21600

# JWT Secret (generate a random string)
JWT_SECRET=your_random_secret_key_here

# Optional: Other concert APIs
# SONGKICK_API_KEY=your_songkick_key_here
# BANDSINTOWN_APP_ID=your_bandsintown_app_id_here
